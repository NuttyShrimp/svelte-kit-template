generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model session {
  id         String   @id
  user_id    String
  expires_at DateTime @db.Timestamptz(6)
  user       user     @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "session_user_id_user_id_fk")
}

model user {
  id        String    @id
  uid       String?   @unique(map: "user_uid_unique")
  firstName String?
  lastName  String?
  email     String?
  session   session[]
}
